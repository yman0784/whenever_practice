<div class="contents row">
  <% @tweets.each do |tweet| %>
    <div class="content_post">
      <p><%= tweet.text %></p>
      <span class="name">
        <%= tweet.name %>
      </span>
    </div>
  <% end %>
</div>
<div class="contents row">
  <% @tvs.each do |tv| %>
  <%= tv.title %>
  <%= l tv.created_at %>
  <% end %>
</div>